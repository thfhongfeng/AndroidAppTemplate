<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android">

    <data class="MvvmTravelNoteDayItemBinding">

        <variable
            name="travelNoteDayDetail"
            type="com.pine.mvvm.bean.MvvmTravelNoteDetailEntity.DayBean"></variable>
    </data>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="#f8f8f8"
        android:orientation="vertical"
        android:paddingLeft="@dimen/dp_10"
        android:paddingRight="@dimen/dp_10">

        <com.pine.base.component.editor.ui.BaseTextImageDisplayView
            android:id="@+id/day_content_tidv"
            contentList="@{travelNoteDayDetail.getTextImageContentList()}"
            title="@{travelNoteDayDetail.day}"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"></com.pine.base.component.editor.ui.BaseTextImageDisplayView>
    </LinearLayout>
</layout>